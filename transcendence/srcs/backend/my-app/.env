# cookie expiration time (in seconds)
JWT_EXPIRATION_TIME=${JWT_EXPIRATION_TIME}

JWT_COOKIE_NAME=${JWT_COOKIE_NAME}

IP_SERVER=${IP_SERVER}
FRONTEND_PORT=${FRONTEND_PORT}
BACKEND_PORT=${BACKEND_PORT}
